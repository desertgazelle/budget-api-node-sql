{
  "name": "budget-api-node-sql",
  "version": "1.0.0",
  "description": "Budget APIs developped with NodeJS, Express and SQL server",
  "main": "index.js",
  "scripts": {
    "start": "nodemon app.js",
    "debug": "cross-env DEBUG=* nodemon app.js",
    "test": "mocha tests/**/*.tests.js --watch",
    "coverage": "nyc npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desertgazelle/budget-api-node-sql.git"
  },
  "author": "Stephanie Boni",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/desertgazelle/budget-api-node-sql/issues"
  },
  "homepage": "https://github.com/desertgazelle/budget-api-node-sql#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "express": "^4.17.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "mssql": "^8.1.0",
    "nodemon": "^2.0.15",
    "uuid": "^8.3.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": 2500,
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    }
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "sinon": "^13.0.2"
  }
}
